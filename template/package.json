  "version": "1.0.0",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/PROJECT_NAME.git"
  },
  "homepage": "https://github.com/newhighsco/PROJECT_NAME#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/PROJECT_NAME/issues"
  },
  "scripts": {
    "test": "npm run lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.jsx,.json .",
    "lint:css": "stylelint '**/*.{css,scss}' --cache",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix",
    "format:css": "npm run lint:css -- --fix"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@newhighsco/commitlint-config": "1.0.7",
    "@newhighsco/editor-config": "1.1.1",
    "@newhighsco/eslint-config": "2.2.58",
    "@newhighsco/prettier-config": "2.0.8",
    "@newhighsco/release-config": "1.0.41",
    "@newhighsco/stylelint-config": "2.0.27",
    "eslint": "7.16.0",
    "husky": "4.3.6",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "semantic-release": "17.3.0",
    "stylelint": "13.8.0"
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ]
  }
}
